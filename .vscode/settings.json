        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/build/**": true,
        "**/logs/**": true,
        "**/tmp/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true,
        "**/build": true
    }
}